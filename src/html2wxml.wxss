.h1 {
  display: block;
  margin-top: 0;
  font-size: 48rpx;
  color: #39f;
  font-weight: 400;
  text-align: justify;
}

.h2 {
  display: block;
  margin-top: 0;
  font-size: 36rpx;
  color: #39f;
  font-weight: 400;
  text-align: justify;
}

.a {
  color: #3399ff;
  text-decoration: underline;
}

.p {
  display: block;
  padding: 0;
  margin-top: 0;
  border-top: none;
  border-left: none;
  border-bottom: none;
  text-align: justify;
  -webkit-margin-before: 1em;
  -webkit-margin-after: 1em;
  -webkit-margin-start: 0;
  -webkit-margin-end: 0;
}

.div {
  display: block;
}

.table {
  display: block;
  width: 100%;
  border-collapse: collapse;
  overflow: auto;
  border-right: 2rpx solid #ededed;
  margin: 20rpx 0 0;
  padding: 0;
  border-top: none;
  border-left: none;
  border-bottom: none;
}

.tbody {
  width: 100%;
  display: table;
  vertical-align: middle;
  border-color: inherit;
}

.tr {
  display: table-row;
  margin: 0;
  padding: 0;
  vertical-align: inherit;
  border-color: inherit;
}

.tr:first-child {
  border-top: 2rpx #ededed solid;
}

.td {
  display: table-cell;
  vertical-align: inherit;
  min-width: 200rpx;
  margin: 0;
  padding: 14rpx 10rpx 10rpx;
  border: 2rpx solid #ededed;
  border-right: none;
  border-top: none;
  font-size: 24rpx;
  text-align: justify;
  color: #666;
}